{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "ourStrapiAuthRootApi": {
      "debug": false,
      "matchCondition": {
        "methods": [
          "POST"
        ],
        "route": "/api/auth"
      },
      "backendUri": "%STRAPI_API_BASE_URL%/api/auth/local"
    },
    "ourStrapiAuthApi": {
      "debug": false,
      "matchCondition": {
        "methods": [
          "POST"
        ],
        "route": "/api/auth/{*restOfPath}"
      },
      "backendUri": "%STRAPI_API_BASE_URL%/api/auth/local/{restOfPath}"
    },
    "ourStrapiAuthGetMeApi": {
      "debug": false,
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/api/me"
      },
      "backendUri": "%STRAPI_API_BASE_URL%/api/me"
    }
  }
}
