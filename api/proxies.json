{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "ourStrapiAuthSignInApi": {
      "debug": true,
      "matchCondition": {
        "methods": [
          "POST"
        ],
        "route": "/api/auth/signin"
      },
      "backendUri": "%STRAPI_API_BASE_URL%/api/auth/local"
    },
    "ourStrapiAuthRegisterInApi": {
      "debug": true,
      "matchCondition": {
        "methods": [
          "POST"
        ],
        "route": "/api/auth/register"
      },
      "backendUri": "%STRAPI_API_BASE_URL%/api/auth/local/register"
    },
    "ourStrapiAuthRemainingApi": {
      "debug": true,
      "matchCondition": {
        "methods": [
          "GET",
          "POST"
        ],
        "route": "/api/auth/{*restOfPath}"
      },
      "backendUri": "%STRAPI_API_BASE_URL%/api/auth/{restOfPath}"
    },
    "ourStrapiAuthGetMeApi": {
      "debug": true,
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/api/me"
      },
      "backendUri": "%STRAPI_API_BASE_URL%/api/users/me"
    }
  }
}
